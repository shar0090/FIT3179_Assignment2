{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 600,
    "projection": {"type":"equalEarth"},
    "layer": [
        {
            "data": {
                "url": "https://raw.githubusercontent.com/shar0090/FIT3179_Assignment2/main/ne_110m%20(3).json",
                "format": {
                    "type": "topojson",
                    "feature": "ne_110m_populated_places"
                }
            },
            "transform": [
                {
                    "lookup": "properties.NAME",
                    "from": {
                        "data": {
                            "url": "https://raw.githubusercontent.com/shar0090/FIT3179_Assignment2/main/Olympic_Swimming_Results_1912to2020%202.csv"
                        },
                        "key": "Location",
                        "fields": ["Rank"]
                    }
                }
            ],
            "mark": {
                "type": "geoshape",
                "fill": "lightgray",
                "stroke": "white"
            }
        }
    ]
}